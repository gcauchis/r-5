{
  "name": "r-5",
  "version": "0.0.1-alpha1",
  "author": "gcauchis",
  "description": "The R-5 army unit builder",
  "main": "main.js",
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.9",
    "@angular/cdk": "^10.2.6",
    "@angular/common": "^10.0.9",
    "@angular/compiler": "^10.0.9",
    "@angular/core": "^10.0.9",
    "@angular/forms": "^10.0.9",
    "@angular/material": "^10.2.6",
    "@angular/platform-browser": "^10.0.9",
    "@angular/platform-browser-dynamic": "^10.0.9",
    "@angular/router": "^10.0.9",
    "html-to-pdfmake": "^2.0.10",
    "pdfmake": "^0.1.68",
    "rxjs": "^6.6.2",
    "tslib": "^2.0.1",
    "zone.js": "^0.10.3"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:electron": "ng build --base-href ./ && electron .",
    "build:electron": "ng build --base-href ./ && electron-builder",
    "build:prod:electron": "ng build --prod --base-href ./ && electron-builder",
    "build:prod:electron:win:linux": "ng build --prod --base-href ./ && electron-builder --linux --win",
    "release": "ng build --prod --base-href ./ && electron-builder"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.3",
    "@angular/cli": "10.1.3",
    "@angular/compiler-cli": "10.1.3",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.9.5",
    "codelyzer": "~4.5.0",
    "electron": "^10.1.2",
    "electron-builder": "^22.8.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "^3.9.7"
  },
  "build": {
    "appId": "com.github.gcauchis.r5",
    "productName": "R-5",
    "files": [
      "main.js",
      "dist/demo"
    ]
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false,
    "constants": false,
    "stream": false
  }
}
